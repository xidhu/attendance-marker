{
  "name": "Attendance-Marker",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Attendance Marker for Google Meet.",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["js/content.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "attendance marker for Google Meet",
    "default_icon": "icons/icon.png"
  },
  "permissions": ["tabs", "cookies"]
}
